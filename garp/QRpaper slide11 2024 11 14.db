:- module('QRpaper slide11 2024 11 14', []).

%!  qspace(?ParameterInstance, ?ParameterDef, ?ValueList, ?Fail).

qspace(x1,x(an_e,x1,continuous,qp),[p],fail).
qspace(y1,y(an_e,y1,continuous,qp),[p],fail).
qspace(z1,z(an_e,z1,continuous,qp),[p],fail).


%!   input_state(-Dict).

input_state(_550{}).


%!   qrel(?Rel).

qrel(prop_pos(y1,x1)).
qrel(prop_neg(y1,z1)).


%!   qstate(?State, ?Values).

qstate(1,_5382{x1:d(p,plus,_5326,_5328),y1:d(p,min,_5348,_5350),z1:d(p,plus,_5370,_5372)}).
qstate(2,_5298{x1:d(p,plus,_5242,_5244),y1:d(p,zero,_5264,_5266),z1:d(p,plus,_5286,_5288)}).
qstate(3,_5382{x1:d(p,plus,_5326,_5328),y1:d(p,plus,_5348,_5350),z1:d(p,plus,_5370,_5372)}).


%!   qstate_from(?State, ?From:list).

qstate_from(2,[1,3,input]).
qstate_from(3,[2,input]).
qstate_from(1,[2,input]).


%!   qstate_to(?State, ?Cause).

qstate_to(1,[to(cause([assumed_derivative_down_to_stable(y1)]),conditions([]),results([par_relations([d_equal(y1,zero)])]),to_state([2]),closed)]).
qstate_to(2,[to(cause([assumed_derivative_stable_to_down(y1)]),conditions([]),results([par_relations([d_smaller(y1,zero)])]),to_state([1]),closed),to(cause([assumed_derivative_stable_to_up(y1)]),conditions([]),results([par_relations([d_greater(y1,zero)])]),to_state([3]),closed)]).
qstate_to(3,[to(cause([assumed_derivative_up_to_stable(y1)]),conditions([]),results([par_relations([d_equal(y1,zero)])]),to_state([2]),closed)]).
